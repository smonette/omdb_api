<!doctype html>
<html>
  <head>
    <title>Search Results</title>
    <link rel="stylesheet" type="text/css" href="/styles/style.css">
  </head>
  <body>
    <div class="container">
      <h1>Results</h1>
      <ul>
        <% movieList.forEach(function (movie) { %>
        <li>
            <a href="movies/<%= movie.imdbID %>"><%= movie.Title %> </a>
        </li>
        <% })%>
      </ul>
    </div>
  </body>
</html>
